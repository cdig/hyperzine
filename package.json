{
  "name": "Hyperzine",
  "version": "0.4.7",
  "license": "UNLICENSED",
  "main": "target/main.js",
  "authors": "Ivan Reese",
  "copyright": "Â© CD Industrial Group Inc.",
  "description": "An asset management tool",
  "scripts": {
    "asset-coffee:assemble": "fd . -e coffee asset | xargs awk '(FNR==1){print \"\\n\\n# \" FILENAME}1' > temp/asset.coffee",
    "asset-coffee:compiler": "coffee --bare --inline-map --output target/asset.js --compile temp/asset.coffee",
    "asset-coffee:generate": "run-s asset-coffee:assemble asset-coffee:compiler",
    "asset-coffee:observer": "onchange 'asset/**/*.coffee' -- npm run asset-coffee:generate",
    "asset-html:generate": "cp asset/asset.html target/asset.html",
    "asset-html:observer": "onchange 'asset/asset.html' -- npm run asset-html:generate",
    "asset-scss:assemble": "fd . -e scss asset | xargs awk '(FNR==1){print \"\\n\\n// \" FILENAME}1' > temp/asset.scss",
    "asset-scss:compiler": "sass temp/asset.scss target/asset.css --no-source-map",
    "asset-scss:generate": "run-s asset-scss:assemble asset-scss:compiler",
    "asset-scss:observer": "onchange 'asset/**/*.scss' -- npm run asset-scss:generate",
    "browser-coffee:assemble": "fd . -e coffee browser | xargs awk '(FNR==1){print \"\\n\\n# \" FILENAME}1' > temp/browser.coffee",
    "browser-coffee:compiler": "coffee --bare --inline-map --output target/browser.js --compile temp/browser.coffee",
    "browser-coffee:generate": "run-s browser-coffee:assemble browser-coffee:compiler",
    "browser-coffee:observer": "onchange 'browser/**/*.coffee' -- npm run browser-coffee:generate",
    "browser-html:generate": "cp browser/browser.html target/browser.html",
    "browser-html:observer": "onchange 'browser/browser.html' -- npm run browser-html:generate",
    "browser-scss:assemble": "fd . -e scss browser | xargs awk '(FNR==1){print \"\\n\\n// \" FILENAME}1' > temp/browser.scss",
    "browser-scss:compiler": "sass temp/browser.scss target/browser.css --no-source-map",
    "browser-scss:generate": "run-s browser-scss:assemble browser-scss:compiler",
    "browser-scss:observer": "onchange 'browser/**/*.scss' -- npm run browser-scss:generate",
    "common-coffee:assemble": "fd . -e coffee common | xargs awk '(FNR==1){print \"\\n\\n# \" FILENAME}1' node_modules/take-and-make/**/*.coffee submodule/bucket/{adsr,monkey-patch,test}.coffee node_modules/doom/doom.coffee lib/*.coffee > temp/common.coffee",
    "common-coffee:compiler": "coffee --bare --inline-map --output target/common.js --compile temp/common.coffee",
    "common-coffee:generate": "run-s common-coffee:assemble common-coffee:compiler",
    "common-coffee:observer": "onchange 'common/**/*.coffee' 'submodule/bucket/{adsr,monkey-patch,test}.coffee' 'lib/*.coffee' -- npm run common-coffee:generate",
    "common-scss:assemble": "fd . -e scss common | xargs awk '(FNR==1){print \"\\n\\n// \" FILENAME}1' submodule/house-style/{vars,reset}.css > temp/common.scss",
    "common-scss:compiler": "sass temp/common.scss target/common.css --no-source-map",
    "common-scss:generate": "run-s common-scss:assemble common-scss:compiler",
    "common-scss:observer": "onchange 'common/**/*.scss' -- npm run common-scss:generate",
    "db-coffee:assemble": "fd . -e coffee db | xargs awk '(FNR==1){print \"\\n\\n# \" FILENAME}1' > temp/db.coffee",
    "db-coffee:compiler": "coffee --bare --inline-map --output target/db.js --compile temp/db.coffee",
    "db-coffee:generate": "run-s db-coffee:assemble db-coffee:compiler",
    "db-coffee:observer": "onchange 'db/**/*.coffee' -- npm run db-coffee:generate",
    "db-html:generate": "cp db/db.html target/db.html",
    "db-html:observer": "onchange 'db/db.html' -- npm run db-html:generate",
    "db-scss:assemble": "fd . -e scss db | xargs awk '(FNR==1){print \"\\n\\n// \" FILENAME}1' > temp/db.scss",
    "db-scss:compiler": "sass temp/db.scss target/db.css --no-source-map",
    "db-scss:generate": "run-s db-scss:assemble db-scss:compiler",
    "db-scss:observer": "onchange 'db/**/*.scss' -- npm run db-scss:generate",
    "main-coffee:assemble": "fd . -e coffee main | xargs awk '(FNR==1){print \"\\n\\n# \" FILENAME}1' node_modules/take-and-make/**/*.coffee submodule/bucket/{adsr,monkey-patch,test}.coffee lib/*.coffee > temp/main.coffee",
    "main-coffee:compiler": "coffee --bare --inline-map --output target/main.js --compile temp/main.coffee",
    "main-coffee:generate": "run-s main-coffee:assemble main-coffee:compiler",
    "main-coffee:observer": "onchange 'main/**/*.coffee' 'submodule/bucket/{adsr,monkey-patch,test}.coffee' 'lib/*.coffee' -- npm run main-coffee:generate",
    "setup-assistant-coffee:assemble": "fd . -e coffee setup-assistant | xargs awk '(FNR==1){print \"\\n\\n# \" FILENAME}1' > temp/setup-assistant.coffee",
    "setup-assistant-coffee:compiler": "coffee --bare --inline-map --output target/setup-assistant.js --compile temp/setup-assistant.coffee",
    "setup-assistant-coffee:generate": "run-s setup-assistant-coffee:assemble setup-assistant-coffee:compiler",
    "setup-assistant-coffee:observer": "onchange 'setup-assistant/**/*.coffee' -- npm run setup-assistant-coffee:generate",
    "setup-assistant-html:generate": "cp setup-assistant/setup-assistant.html target/setup-assistant.html",
    "setup-assistant-html:observer": "onchange 'setup-assistant/setup-assistant.html' -- npm run setup-assistant-html:generate",
    "setup-assistant-scss:assemble": "fd . -e scss setup-assistant | xargs awk '(FNR==1){print \"\\n\\n// \" FILENAME}1' > temp/setup-assistant.scss",
    "setup-assistant-scss:compiler": "sass temp/setup-assistant.scss target/setup-assistant.css --no-source-map",
    "setup-assistant-scss:generate": "run-s setup-assistant-scss:assemble setup-assistant-scss:compiler",
    "setup-assistant-scss:observer": "onchange 'setup-assistant/**/*.scss' -- npm run setup-assistant-scss:generate",
    "deploy": "electron-forge publish --arch=x64,arm64",
    "deps": "cp node_modules/d3-color/dist/d3-color.min.js target",
    "static": "cp -R static target/static",
    "bump": "yarn version --patch --no-git-tag-version",
    "open": "open ./Hyperzine-darwin-arm64/Hyperzine.app",
    "clean": "rm -r target temp",
    "init": "mkdir -p target temp",
    "electron": "electron .",
    "build": "npm-run-all init deps static *:generate",
    "watch": "npm-run-all --parallel *:observer",
    "start": "npm-run-all build watch"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "split-grid": "^1.0.11"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.60",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.60",
    "@electron-forge/maker-zip": "^6.0.0-beta.60",
    "@electron-forge/publisher-github": "^6.0.0-beta.60",
    "DOOM": "ivanreese/doom",
    "coffeescript": "^2.5.1",
    "d3-color": "^3.0.1",
    "dotenv": "^10.0.0",
    "electron": "^15.0.0-alpha.10",
    "npm-run-all": "latest",
    "onchange": "latest",
    "sass": "^1.37.5",
    "take-and-make": "cdig/take-and-make"
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
