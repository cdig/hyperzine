{
  "name": "Hyperzine",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "main": "build/main/main.js",
  "scripts": {
    "build": "coffee --bare --inline-map --join build/app.js --watch --compile node_modules/take-and-make/source/take-and-make.coffee node_modules/doom/doom.coffee coffee/",
    "start": "electron-forge start -i"
  },
  "dependencies": {
    "doom": "ivanreese/doom",
    "electron-compile": "^6.4.3",
    "preact": "latest",
    "take-and-make": "cdig/take-and-make"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.7"
  },
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "packageManager": "yarn"
      }
    }
  }
}
