Take [], ()->
  offset = .2 + .4 * Math.random() + (if Math.random() < .5 then 9.2 else 0)

  src = "data:image/svg+xml;base64,"

  recurse = (count)->
    Preact.h "div", {style: "animation-delay: -#{offset}s"},
      Preact.h "img", src: src
      recurse count if --count > 0

  gears = recurse 30

  Make "GearView", ()->
    Preact.h "gear-view", null, gears
